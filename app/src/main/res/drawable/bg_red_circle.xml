<layer-list xmlns:android="http://schemas.android.com/apk/res/android">


    <item
        android:width="64dp"
        android:height="64dp">
        <shape android:shape="oval">
            <solid android:color="@android:color/white" />
        </shape>
    </item>

    <item
        android:bottom="4dp"
        android:end="4dp"
        android:start="4dp"
        android:top="4dp">
        <shape android:shape="oval">
            <solid android:color="@android:color/holo_red_dark" />
        </shape>
    </item>

</layer-list>
